{
	"presets": [
		"@babel/preset-react",
		[
			"@babel/preset-env",
			{
				"useBuiltIns": "entry",
				"corejs": 3,
				"modules": false,
				"targets": {
					"browsers": [
						"> 1%",
						"last 2 versions",
						"not ie <= 8"
					]
				}
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-syntax-dynamic-import",
		[
			"import",
			{
				"libraryName": "antd",
				"libraryDirectory": "es",
				"style": false
			}
		]
	]
}
